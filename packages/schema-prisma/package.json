{
    "name": "@feuertiger/schema-prisma",
    "version": "1.0.9-dev.6dbe3904.0",
    "description": "prisma schema",
    "main": "./dist/index.js",
    "scripts": {
        "build": " prisma generate --schema=./src/schema.prisma",
        "migrate:save": "prisma migrate save --experimental --name=$(git rev-parse --short HEAD) --schema=./src/schema.prisma",
        "migrate:up": "prisma migrate up --experimental --create-db --schema=./src/schema.prisma",
        "seed:test": "ts-node ./seeds/test/seed.ts",
        "dev": "yarn migrate:save && yarn migrate:up && yarn build && yarn seed:test && yarn build --watch"
    },
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/feuertiger/feuertiger",
        "directory": "packages/schema-prisma"
    },
    "publishConfig": {
        "@feuertiger:registry": "https://gitlab.com/api/v4/projects/18726494/packages/npm/"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "2.0.0",
        "faker": "4.1.0"
    },
    "devDependencies": {
        "@prisma/cli": "2.0.0",
        "ts-node": "8.10.1"
    },
    "gitHead": "7d224ef51bbc23af4badd7b61540546319995beb"
}
